        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bigsegs.SafeJourney"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}